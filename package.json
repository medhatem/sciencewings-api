{
  "name": "manaservices_server",
  "version": "1.0.0",
  "description": "Series of Restful micro-services with NodeJs+Express",
  "main": "dist/server/index.js",
  "module": "dist/client/index.js",
  "types": "dist/server/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "init": "rm -rf dist/ && npm run clean && npm i && npm run build",
    "build": "tsc -p src/server && tsc -p src/client && tsc -p src/server-unit",
    "watch": " concurrently --names 'server,client,server-unit' -c 'bgBlue,black.bgGreen,black.bgYellow' 'tsc -p src/server --watch' 'tsc -p src/client --watch' 'tsc -p src/server-unit --watch'    ",
    "mongo-start": "docker-compose -f mongodb.yml up -d",
    "mongo-stop": "docker-compose -f mongodb.yml down",
    "swagger-gen": "node node_modules/typescript-rest-swagger/dist/cli.js -c ./swaggerConfig.json",
    "dev": "npm run swagger-gen ; nodemon --ignore 'dist/**/swagger.json' --ignore 'dist/**/swagger.yaml' ./dist/server/index.js",
    "start": "node ./dist/server/index.js",
    "test": "intern config=src/server-unit/intern.json",
    
  },
  "_moduleAliases": {
    "@di": "dist/server/di",
    "@types": "dist/server/types",
    "@errors": "dist/server/errors",
    "@interface": "dist/server/interface/*",
    "@models": "dist/server/models",
    "@routes": "dist/server/routes",
    "@service": "dist/server/service",
    "@dao": "dist/server/dao"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdesselamXpack94/manaServices_server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abdesselamXpack94/manaServices_server/issues"
  },
  "homepage": "https://github.com/abdesselamXpack94/manaServices_server#readme",
  "dependencies": {
    "@typegoose/typegoose": "^7.6.1",
    "bcrypt": "latest",
    "body-parser": "latest",
    "cors": "latest",
    "express": "latest",
    "inversify": "latest",
    "inversify-binding-decorators": "latest",
    "joi": "latest",
    "jsonwebtoken": "latest",
    "module-alias": "^2.2.2",
    "mongoose": "5.10.18",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "latest",
    "swagger-ui-express": "^4.1.4",
    "typescript-rest-swagger": "^1.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "latest",
    "@types/body-parser": "latest",
    "@types/cors": "latest",
    "@types/express": "latest",
    "@types/joi": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/mongoose": "5.10.2",
    "@types/sinon": "latest",
    "@types/passport-jwt": "^3.0.1",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "concurrently": "latest",
    "dotenv": "latest",
    "eslint": "latest",
    "eslint-config-google": "latest",
    "intern": "latest",
    "nodemon": "latest",
    "sinon": "latest",
    "typescript": "latest"
  }
}
