{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules" : {
        "require-jsdoc" : 0,
        "max-len": ["error", { "code": 120 }],
        "indent":"off",
        "object-curly-spacing":"off",
        "new-cap":"off",
        "valid-jsdoc":"off",
        "space-before-function-paren":"off"

    }
}
