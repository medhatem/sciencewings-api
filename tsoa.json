{
    "entryFile": "src/server/server.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["src/**/routes/index.ts"],
    "spec": {
      "outputDirectory": "src/server/tsoa-build",
      "specVersion": 3,
      "basePath": "/api/v1",
      "specMerging": "recursive"
    },
    "routes": {
      "routesDir": "src/server/tsoa-build",
      "basePath": "/api/v1",
      "iocModule": "src/server/di/index.ts",
      "middlewareTemplate": "custom-template.ts"
      
    }
  }